{
  "name": "kafka-node",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:dhanielsales/kafka-node.git",
  "author": "Dhanielr <dhanielr@hotmail.com>",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "dev:user-service": "ts-node-dev --watch --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/user-service/index.ts",
    "dev:lead-service": "ts-node-dev --watch --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/lead-service/index.ts",
    "start:user-service": "node build/user-service/index.ts",
    "start:lead-service": "node build/lead-service/index.ts",
    "build": "tsc --project tsconfig.json",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "kafkajs": "^2.2.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^14.14.22",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
